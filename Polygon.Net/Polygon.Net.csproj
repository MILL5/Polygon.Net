<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
	<Version>1.0.0-alpha</Version>
	<AssemblyVersion>1.0.0.0</AssemblyVersion>
	<FileVersion>1.0.0.0</FileVersion>
    <Authors>MILL5</Authors>
    <Description>A .NET class library for use against the Polygon APIs supporting .NET Standard, .NET Core, and the new .NET 5.0</Description>
	<License>https://www.gnu.org/licenses/gpl-3.0.en.html</License>
    <Copyright>Copyright Â© MILL5, LLC 2021</Copyright>
    <PackageProjectUrl>https://github.com/MILL5/Polygon.Net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MILL5/Polygon.Net</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>stocks,financial,polygon,price,api</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PublishRepositoryUrl>true</PublishRepositoryUrl>
	<IncludeSymbols>true</IncludeSymbols>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="5.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Pineapple" Version="1.0.30" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Solution Items\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\.gitignore" Link="Solution Items\.gitignore" />
    <None Include="..\Polygon.Net.runsettings" Link="Solution Items\Polygon.Net.runsettings" />
  </ItemGroup>

</Project>
